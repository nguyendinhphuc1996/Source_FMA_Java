package MyMethods;

import java.text.SimpleDateFormat;
import java.util.Date;

public class MyMethods
{
	public boolean checkStringIsDate(String $string) // Kiểm tra chuỗi phải là Date
	{
		SimpleDateFormat sdf = new SimpleDateFormat("dd/MM/yyyy");
		try
		{
			Date $date = sdf.parse($string);
			return true;
		}catch(Exception e)
		{
			return false;
		}
	}
	
	public Date getDateFromString(String $string) // Chuyển đổi chuối sang Date
	{
		Date $date;
		SimpleDateFormat sdf = new SimpleDateFormat("dd/MM/yyyy");
		try
		{
			$date = sdf.parse($string);
		}catch(Exception e)
		{
		}
		return $date;
	}
}
