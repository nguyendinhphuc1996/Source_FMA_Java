package phuc;

public class BaiTap1
{
	public static void main(String[] args)
	{
		System.out.println("========== Ví dụ 1 ===========");
		Customer c = new Customer();
		c.customerID = 4496;
		c.customerName = "Phuc";
		c.customerAddress = "HCM City";
		c.customerAge = 21;
	}
}

class Customer
{
	int customerID;
	String customerName;
	String customerAddress;
	int customerAge;
}
