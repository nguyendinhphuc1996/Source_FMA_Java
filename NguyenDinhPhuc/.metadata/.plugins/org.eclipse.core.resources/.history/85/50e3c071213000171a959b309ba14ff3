	{
		System.out.println("*************** MENU ***************");
		System.out.println("* 1. add -> Input                  *");
		System.out.println("* 2. disp -> Display               *");
		System.out.println("* 3. exit -> Exit application      *");
		System.out.println("************************************");
	}
}

