package TOPIC_2;
import static java.lang.System.*; // import tất cả trường và method trong Class System là Static

// 1. Nhập vào một xâu. Tạo ra xâu đảo ngược với xâu đã nhập.

public class BaiTap_1 
{
	public static void main(String[] args) 
	{
		out.println("it is working");
		out.print("HAHA");
	}

}
