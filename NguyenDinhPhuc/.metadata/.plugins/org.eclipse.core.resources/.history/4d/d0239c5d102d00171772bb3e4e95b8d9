package TOPIC_2;

public class BaiTap_3
{
	public static void main(String[] args)
	{

		double x = 4.53;
		double x = 4.5;
		if ((x%1) == 0)
			System.out.println("same");
		else
			System.out.println("different");
		
		if ((Math.round(x)%1) == 0)
			System.out.println("same");
		else
			System.out.println("different");
	}
}
