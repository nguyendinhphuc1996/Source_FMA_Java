package TOPIC_2;

public class BaiTap_3
{
	public static void main(String[] args)
	{

		float x = 4.34f;
		float y = 4f;
		if ((x%1) == 0)
			System.out.println("same");
		else
			System.out.println("different");
		
		if ((int)x == (int)y)
			System.out.println("same");
		else
			System.out.println("different");
	}
}
