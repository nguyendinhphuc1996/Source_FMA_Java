package TOPIC_2;

public class BaiTap_3
{
	public static void main(String[] args)
	{
		float f1 = 3.2f;
		float f2 = 6.4f;

		if ((int)f1 == 3)
			System.out.println("same");
		else
			System.out.println("different");

		if (f2 == 6.4f)
			System.out.println("same");
		else
			System.out.println("different");
	}
}
