package TOPIC_2;

public class BaiTap_3
{
	public static void main(String[] args)
	{

		double x = 4.53;
		while (x % 1 != 0)
		{
			x = (Math.round(x*10000)/10000)*10;
			if ((x % 1) == 0)
				System.out.print("same");
			else
				System.out.print("different");
			System.out.println(" -> Giá trị x: "+x);
		}
	}
}
